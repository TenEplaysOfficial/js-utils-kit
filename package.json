{
  "name": "js-utils-kit",
  "version": "0.0.0",
  "description": "Essential JavaScript helpers",
  "license": "MIT",
  "private": false,
  "repository": "https://github.com/TenEplaysOfficial/js-utils-kit",
  "bugs": {
    "url": "https://github.com/TenEplaysOfficial/js-utils-kit/issues"
  },
  "author": {
    "name": "Sriman",
    "email": "136729116+TenEplaysOfficial@users.noreply.github.com",
    "url": "https://tene.vercel.app"
  },
  "keywords": [
    "javascript",
    "utils",
    "helpers",
    "toolkit",
    "typescript",
    "promise",
    "array",
    "object",
    "small"
  ],
  "type": "module",
  "files": [
    "dist/**/*"
  ],
  "bin": {
    "js-utils-kit": "dist/index.js"
  },
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.esm.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs.js"
      }
    }
  },
  "lint-staged": {
    "*.{js,mjs,ts,json,md,yml}": "yarn format"
  },
  "scripts": {
    "build": "rollup -c && yarn types",
    "release": "release-it",
    "types": "tsc -p tsconfig.types.json",
    "watch": "rollup -c --watch",
    "lint": "eslint .",
    "eslint:fix": "npx eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@eslint/json": "^0.12.0",
    "@eslint/markdown": "^6.6.0",
    "@release-it/conventional-changelog": "^10.0.1",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.3",
    "eslint": "^9.29.0",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.0",
    "release-it": "^19.0.3",
    "rollup": "^4.44.0",
    "rollup-plugin-delete": "^3.0.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.0"
  }
}
